  "flow": {
    "triggers": [
      {
        "id": "trigger_NTFY_BACKGROUND_ACTIVE",
        "title": {
          "en": "Background movement Detected",
          "nl": "Achtergrond Beweging Gedetecteerd"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=NTFY_BACKGROUND_ACTIVE"
          }
        ]
      },
      {
        "id": "trigger_NTFY_MOTION_DETECTION",
        "title": {
          "en": "Motion Detected",
          "nl": "Beweging Gedetecteerd"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=NTFY_MOTION_DETECTION"
          }
        ]
      },
      {
        "id": "trigger_NTFY_FACE_DETECTION",
        "title": {
          "en": "Person Detected",
          "nl": "Persoon Gedetecteerd"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=NTFY_FACE_DETECTION"
          }
        ]
      },
      {
        "id": "trigger_NTFY_PRESS_DOORBELL",
        "title": {
          "en": "Doorbell Press Detected",
          "nl": "Deurbel Druk Gedetecteerd"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=NTFY_PRESS_DOORBELL"
          }
        ]
      },
      {
        "id": "trigger_NTFY_CRYING_DETECTED",
        "title": {
          "en": "Crying Detected",
          "nl": "Huilen Gedetecteerd"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=NTFY_CRYING_DETECTED"
          }
        ]
      },
      {
        "id": "trigger_NTFY_SOUND_DETECTED",
        "title": {
          "en": "Sound Detected",
          "nl": "Geluid Gedetecteerd"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=NTFY_SOUND_DETECTED"
          }
        ]
      },
      {
        "id": "trigger_NTFY_PET_DETECTED",
        "title": {
          "en": "Pet Detected",
          "nl": "Huisdier Gedetecteerd"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=NTFY_PET_DETECTED"
          }
        ]
      }
    ],
    "actions": [
      {
        "id": "action_CMD_SET_ARMING",
        "title": {
          "en": "Set Arm Mode",
          "nl": "Stel beveiligingsmodus in"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=CMD_SET_ARMING"
          },
          {
            "name": "action_CMD_SET_ARM_TYPE",
            "type": "dropdown",
            "values": [
              {
                "id": "1",
                "label": {
                  "en": "Home",
                  "nl": "Home"
                }
              },
              {
                "id": "0",
                "label": {
                  "en": "Away",
                  "nl": "Away"
                }
              },
              {
                "id": "2",
                "label": {
                  "en": "Schedule",
                  "nl": "Schedule"
                }
              },
              {
                "id": "63",
                "label": {
                  "en": "Disarmed",
                  "nl": "Disarmed"
                }
              }
            ]
          }
        ]
      },
      {
        "id": "action_CMD_DOORBELL_QUICK_RESPONSE",
        "title": {
          "en": "Send Quick Response",
          "nl": "Stuur Spraakbericht"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=CMD_DOORBELL_QUICK_RESPONSE"
          },
          {
            "name": "action_CMD_SET_QUICK_RESPONSE_TYPE",
            "type": "dropdown",
            "values": [
              {
                "id": "1",
                "label": {
                  "en": "Default response 1",
                  "nl": "Standaard bericht 1"
                }
              },
              {
                "id": "2",
                "label": {
                  "en": "Default response 2",
                  "nl": "Standaard bericht 2"
                }
              },
              {
                "id": "3",
                "label": {
                  "en": "Default response 3",
                  "nl": "Standaard bericht 3"
                }
              },
              {
                "id": "4",
                "label": {
                  "en": "Custom response 1",
                  "nl": "Eigen bericht 1"
                }
              },
              {
                "id": "5",
                "label": {
                  "en": "Custom response 2",
                  "nl": "Eigen bericht 2"
                }
              },
              {
                "id": "6",
                "label": {
                  "en": "Custom response 3",
                  "nl": "Eigen bericht 3"
                }
              }
            ]
          }
        ]
      }
    ]

  "flow": {
    "triggers": [
      {
        "id": "trigger_NTFY_BACKGROUND_ACTIVE",
        "title": {
          "en": "Background movement Detected",
          "nl": "Achtergrond Beweging Gedetecteerd"
        },
        "args": [
           {
                "name": "device",
                "type": "device",
                "filter": "driver_uri=homey:app:com.eufylife.security"
            }
        ]
      },
      {
        "id": "trigger_NTFY_MOTION_DETECTION",
        "title": {
          "en": "Motion Detected",
          "nl": "Beweging Gedetecteerd"
        },
        "args": [
           {
                "name": "device",
                "type": "device",
                "filter": "driver_uri=homey:app:com.eufylife.security"
            }
        ]
      },
      {
        "id": "trigger_NTFY_FACE_DETECTION",
        "title": {
          "en": "Face Detected",
          "nl": "Gezicht Gedetecteerd"
        },
        "args": [
           {
                "name": "device",
                "type": "device",
                "filter": "driver_uri=homey:app:com.eufylife.security"
            }
        ]
      },
      {
        "id": "trigger_NTFY_PRESS_DOORBELL",
        "title": {
          "en": "Doorbell Press Detected",
          "nl": "Deurbel Druk Gedetecteerd"
        },
        "args": [
           {
                "name": "device",
                "type": "device",
                "filter": "driver_uri=homey:app:com.eufylife.security"
            }
        ]
      },
      {
        "id": "trigger_NTFY_CRYING_DETECTED",
        "title": {
          "en": "Crying Detected",
          "nl": "Huilen Gedetecteerd"
        },
        "args": [
           {
                "name": "device",
                "type": "device",
                "filter": "driver_uri=homey:app:com.eufylife.security"
            }
        ]
      },
      {
        "id": "trigger_NTFY_SOUND_DETECTED",
        "title": {
          "en": "Sound Detected",
          "nl": "Geluid Gedetecteerd"
        },
        "args": [
           {
                "name": "device",
                "type": "device",
                "filter": "driver_uri=homey:app:com.eufylife.security"
            }
        ]
      },
      {
        "id": "trigger_NTFY_PET_DETECTED",
        "title": {
          "en": "Pet Detected",
          "nl": "Huisdier Gedetecteerd"
        },
        "args": [
            {
                "name": "device",
                "type": "device",
                "filter": "driver_uri=homey:app:com.eufylife.security"
            }
        ]
      }
    ],
    "actions": [
      {
        "id": "action_CMD_SET_ARMING",
        "title": {
          "en": "Set Arm Mode",
          "nl": "Stel beveiligingsmodus in"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "capabilities=CMD_SET_ARMING"
          },
          {
            "name": "action_CMD_SET_ARM_TYPE",
            "type": "dropdown",
            "values": [
              {
                "id": "1",
                "label": {
                  "en": "Home",
                  "nl": "Home"
                }
              },
              {
                "id": "0",
                "label": {
                  "en": "Away",
                  "nl": "Away"
                }
              },
              {
                "id": "2",
                "label": {
                  "en": "Schedule",
                  "nl": "Schedule"
                }
              },
              {
                "id": "63",
                "label": {
                  "en": "Disarmed",
                  "nl": "Disarmed"
                }
              }
            ]
          }
        ]
      }
    ]
  },
